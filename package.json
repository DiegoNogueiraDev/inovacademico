{
    "name": "devnogueira",
    "version": "1.0.0",
    "description": "Projeto de desenvolvimento profissional com Next.js e MongoDB",
    "main": "index.js",
    "directories": {
        "lib": "lib"
    },
    "scripts": {
        "predev": "node ./scripts/check-mongo.js",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "db:seed": "node ./scripts/seed-database.js",
        "db:migrate": "node ./scripts/migrate-database.js",
        "analyze": "ANALYZE=true next build",
        "prepare": "husky install",
        "format": "prettier --write ."
    },
    "keywords": [
        "nextjs",
        "mongodb",
        "react",
        "tailwindcss"
    ],
    "author": "Diego Nogueira",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "mongodb": "^6.14.2",
        "next": "^15.2.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "dotenv": "^16.0.3",
        "mongoose": "^7.0.0",
        "axios": "^1.3.4"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.15",
        "postcss": "^8.4.29",
        "tailwindcss": "^3.3.3",
        "jest": "^29.5.0",
        "prettier": "^2.8.4",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ]
    }
}
